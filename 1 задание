#работа с числами 
число=576
a=число+75
b=число-75
c=число//2
d=число*10
e=число**2
print(a,b,c,d,e)

#работа с строками и словами
s1=an
s2=ne
print(s1+s2)
print('ann'*3)
print(len('ann'))
s=anne
print(s[0])

#Работа с списками и кортежами 
m = [2, 1, 3, 7, 5]
print(m)
m.sort()
print(m)
m.reverse()
print(m)
m.append(1)
print(m)
print(m.count(1))
m.remove(2)
print(m)
